<!DOCTYPE HTML PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN"
	"http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
	<title>Sharewood.hu</title>
	<%= stylesheet_link_tag 'style', 'pagination', 'jquery.tooltip'%>
	<link rel="alternate" type="application/rss+xml" title="Sharewood.hu feed" href="http://sharewood.hu/rss.xml" /> 
	<%= javascript_include_tag 'jquery-1.7.1.min', 'jquery.dimensions', 'jquery.bgiframe', 'jquery.tooltip', 'jquery.hotkeys.js' %>
	<link href="/facebox/facebox.css" media="screen" rel="stylesheet" type="text/css" />
	<script src="/facebox/facebox.js" type="text/javascript"></script>

	<script type="text/javascript">
	jQuery(document).ready(function($) {
		$('a[rel*=facebox]').facebox({
			loading_image : 'loading.gif',
			close_image   : 'closelabel.gif'
		}) 
	})
	</script>

</head>
<body>
	<%= yield %>
</body>
</html>
