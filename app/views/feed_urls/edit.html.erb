<h1>Editing feed_url</h1>

<%= error_messages_for :feed_url %>

<% form_for(@feed_url) do |f| %>
  <p>
    <b>Feed url</b><br />
    <%= f.text_field :feed_url %>
  </p>

  <p>
    <b>Star</b><br />
    <%= f.text_field :star %>
  </p>

  <p>
    <%= f.submit "Update" %>
  </p>
<% end %>

<%= link_to 'Show', @feed_url %> |
<%= link_to 'Back', feed_urls_path %>
