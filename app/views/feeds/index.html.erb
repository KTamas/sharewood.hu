<h1>Listing feeds</h1>

<table>
  <tr>
    <th>Site url</th>
    <th>Feed url</th>
    <th>Title</th>
  </tr>

  <% for feed in @feeds %>
    <tr>
      <td><%=h feed.site_url %></td>
      <td><%=h feed.url %></td>
      <td><%=h feed.title %></td>
      <td><%= link_to 'Show', feed %></td>
      <td><%= link_to 'Edit', edit_feed_path(feed) %></td>
      <td><%= button_to "delete", feed, :method=>:delete, :class=>:destroy %></td>
    </tr>
  <% end %>
</table>

<br />

<%= link_to 'New feed', new_feed_path %>
